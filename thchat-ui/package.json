{
  "name": "THChatUI",
  "version": "3.0.2",
  "author": "Unagi-cq <baiwen_88@qq.com>",
  "description": "THChatUI 是一个基于 Vue2 + ElementUI 的LLM WebUI。",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:github": "DEPLOY_ENV=GH_PAGES vue-cli-service build",
    "electron": "electron .",
    "electron:dev": "npm run build && electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@kangc/v-md-editor": "^2.3.18",
    "@langchain/core": "^0.3.50",
    "@langchain/tavily": "^0.1.1",
    "@microsoft/fetch-event-source": "^2.0.1",
    "crypto-js": "^4.2.0",
    "element-plus": "^2.7.0",
    "live2d-helper": "^1.0.1",
    "mammoth": "^1.9.0",
    "marked": "^15.0.6",
    "mitt": "^3.0.1",
    "openai": "^4.77.4",
    "pdfjs-dist": "^5.2.133",
    "segmentit": "^2.0.3",
    "vue": "^3.2.13",
    "vue-i18n": "^11.0.1",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-class-static-block": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@types/cacheable-request": "^6.0.3",
    "@types/http-cache-semantics": "^4.0.4",
    "@types/responselike": "^1.0.3",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-router": "~5.0.0",
    "@vue/cli-plugin-vuex": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "electron": "^36.1.0",
    "electron-builder": "^26.0.12",
    "raw-loader": "^4.0.2",
    "sass": "^1.32.7",
    "sass-loader": "^12.0.0",
    "svg-sprite-loader": "^6.0.11"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "main": "electron-main.js",
  "build": {
    "appId": "com.unagi.cq.thchatui",
    "productName": "THChatUI",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "dist/**/*",
      "electron-main.js",
      "preload.js",
      "assets/**/*"
    ],
    "win": {
      "icon": "public/ico/win_icon_256.ico",
      "target": [
        "nsis"
      ]
    }
  }
}
